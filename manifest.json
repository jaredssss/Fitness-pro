{
  "manifest_version": 3,
  "name": "Fitness Timer Pro",
  "version": "1.0",
  "description": "Track your workouts with built-in timer and progress tracking",
  "permissions": ["storage", "alarms", "notifications"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
